{
  "name": "better-bookmark",
  "version": "0.0.1",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "manifest": {
    "permissions": [
      "activeTab",
      "bookmarks"
    ],
    "commands": {
      "_execute_action": {
        "suggested_key": {
          "default": "Ctrl+B",
          "mac": "Command+B"
        }
      }
    }
  },
  "dependencies": {
    "plasmo": "0.85.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-select": "^5.8.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  }
}
